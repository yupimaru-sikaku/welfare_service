<h2>Sign up</h2>

<%= form_with model: @user, url: user_registration_path, local: true do |f| %>

  
  <%= f.label :nickname %>
  <%= f.text_field :nickname %>

  <%= f.label :email %>
  <%= f.email_field :email %>
  
  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit "送信"%>

<% end %>